    "uniform:push": "uniform context signal push ./content/signals",
    "start": "next start"
  },
  "dependencies": {
    "@uniformdev/context": "^18.0.0",
    "@uniformdev/context-react": "^18.0.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@uniformdev/cli": "^18.0.0",
    "npm-run-all": "^4.1.5"
  }
}
