{
  "name": "intro-to-context",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-s download:manifest next:dev",
    "next:dev": "next dev",
    "build": "run-s download:manifest next:build",
    "next:build": "next build",
    "download:manifest": "uniform context manifest download --output ./contextManifest.json",
