{
	"name": "@uniformdev/sveltekit-starter",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"app:dev": "vite dev",
		"dev": "pnpm pull:manifest && pnpm app:dev",
		"build": "pnpm pull:manifest && pnpm app:build",
		"app:build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"pull:manifest": "uniform context manifest download --output ./src/lib/uniform/contextManifest.json",
		"pull:content": "uniform sync pull",
		"push:content": "uniform sync push"
	},
	"dependencies": {
		"@uniformdev/canvas": "20.48.1-alpha.27",
		"@uniformdev/canvas-svelte": "20.48.1-alpha.27",
		"@uniformdev/canvas-sveltekit": "20.48.1-alpha.27",
		"@uniformdev/context": "20.48.1-alpha.27",
		"@uniformdev/context-svelte": "20.48.1-alpha.27",
		"@uniformdev/context-edge": "20.48.1-alpha.27",
		"@uniformdev/context-edge-sveltekit": "20.48.1-alpha.27",
		"@vercel/functions": "^3.4.0",
		"cookie": "1.0.1",
		"immer": "10.1.3"
	},
	"devDependencies": {
		"@uniformdev/cli": "20.48.1-alpha.27",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-vercel": "^6.3.1",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^25.3.0",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	}
}