{
  "name": "sd-joyride-web",
  "version": "1.0.0",
  "private": true,
  "description": "sd-joyride-web",
  "author": "William Imoh",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "run-s uniform:download-manifest gatsby:develop",
    "gatsby:develop": "gatsby develop",
    "gatsby:build": "gatsby build",
    "build": "run-s build:plugin uniform:download-manifest gatsby:build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "build:plugin": "cd ../uniform-source-plugin && npm install",
    "uniform:fetch-map": "uniform project-map definition list",
    "uniform:download-manifest": "uniform context manifest download --output ./uniform-manifest.json",
    "uniform:context:publish": "uniform context manifest publish",
    "uniform:pull:signals": "uniform context signal pull ./content/signals",
    "uniform:pull:tests": "uniform context test pull ./content/tests",
    "uniform:pull:components": "uniform canvas component pull ./content/components",
    "uniform:pull:compositions": "uniform canvas composition pull ./content/compositions",
    "uniform:pull:project-map-definitions": "uniform project-map definition pull ./content/maps",
    "uniform:pull:project-map-nodes": "uniform project-map node pull ./content/nodes",
    "uniform:pull": "run-s uniform:pull:signals uniform:pull:tests uniform:pull:components uniform:pull:compositions uniform:pull:project-map-definitions uniform:pull:project-map-nodes",
    "uniform:push:signals": "uniform context signal push ./content/signals",
    "uniform:push:tests": "uniform context test push ./content/tests",
    "uniform:push:components": "uniform canvas component push ./content/components",
    "uniform:push:compositions": "uniform canvas composition push ./content/compositions",
    "uniform:push:project-map-definitions": "uniform project-map definition push ./content/maps",
    "uniform:push:project-map-nodes": "uniform project-map node push ./content/nodes",
    "uniform:publish": "uniform canvas composition push ./content/compositions --state published",
    "uniform:push": "run-s uniform:push:signals uniform:push:tests uniform:push:components uniform:push:compositions uniform:push:project-map-definitions uniform:push:project-map-nodes uniform:context:publish"
  },
  "dependencies": {
    "@uniformdev/canvas": "^19.48.0",
    "@uniformdev/canvas-react": "^19.48.0",
    "@uniformdev/cli": "^19.57.0",
    "@uniformdev/context": "^19.48.0",
    "@uniformdev/context-react": "^19.48.0",
    "@uniformdev/project-map": "^19.48.0",
    "dotenv": "^16.0.3",
    "gatsby": "^5.10.0",
    "gatsby-plugin-gatsby-cloud": "^5.10.0",
    "gatsby-plugin-google-analytics": "^5.10.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-manifest": "^5.10.0",
    "gatsby-plugin-sharp": "^5.10.0",
    "gatsby-plugin-sitemap": "^6.2.0",
    "gatsby-source-filesystem": "^5.10.0",
    "gatsby-transformer-sharp": "^5.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "autoprefixer": "^10.4.14",
    "gatsby-plugin-netlify": "^5.1.0",
    "gatsby-plugin-postcss": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.3",
    "vite": "^4.3.7"
  }
}
