{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "run-s context:manifest gatsby:build",
    "develop": "run-s context:manifest gatsby:develop",
    "gatsby:build": "gatsby build",
    "gatsby:develop": "gatsby develop",
    "context:manifest": "uniform context manifest download --output ./src/lib/uniform/manifest.json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@react-hook/window-scroll": "^1.3.0",
    "@uniformdev/canvas": "^18.2.0",
    "@uniformdev/canvas-react": "^18.2.0",
    "@uniformdev/context": "^18.2.0",
    "@uniformdev/context-react": "^18.2.0",
    "cookie": "^0.5.0",
    "gatsby": "^4.19.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@uniformdev/cli": "^18.2.0",
    "autoprefixer": "^10.4.7",
    "gatsby-plugin-postcss": "^5.19.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
