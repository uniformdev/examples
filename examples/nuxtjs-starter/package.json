{
  "name": "@uniformdev/nuxtjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-s download:manifest nuxt:dev",
    "nuxt:dev": "nuxt dev",
    "build": "run-s download:manifest nuxt:build",
    "nuxt:build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "nuxt start",
    "download:manifest": "uniform context manifest download --output ./lib/uniform/contextManifest.json",
    "uniform:pull": "uniform sync pull",
    "uniform:push": "uniform sync push",
    "uniform:publish": "uniform context manifest publish",
    "start:stackblitz": "npm install && nuxt dev",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@uniformdev/cli": "20.35.0",
    "npm-run-all": "4.1.5",
    "nuxt": "^4.1.2",
    "vue": "^3.5.21"
  },
  "dependencies": {
    "@uniformdev/canvas": "20.35.0",
    "@uniformdev/canvas-vue": "20.35.0",
    "@uniformdev/context": "20.35.0",
    "@uniformdev/context-vue": "20.35.0",
    "@uniformdev/project-map": "20.35.0",
    "@uniformdev/uniform-nuxt": "20.35.0"
  },
  "stackblitz": {
    "startCommand": "npm run start:stackblitz",
    "compileTrigger": "save"
  }
}
